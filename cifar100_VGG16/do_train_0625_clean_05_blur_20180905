# train 0.125 from pruned, qunted half
python3 -W ignore network_gen2.py --block1 ckpt_20180905_0625_clean.t0 --o ckpt_20180905_0625_clean_05_blur.t0 --mode 3
python3 -W ignore main_0625_clean_05_blur_20180905.py --lr 0.8 --bs 1024 --mode 1 --ne 50 -r 
python3 -W ignore main_0625_clean_05_blur_20180905.py --lr 0.08 --bs 1024 --mode 1 --ne 80 -r 
python3 -W ignore main_0625_clean_05_blur_20180905.py --lr 0.008 --bs 1024 --mode 1 --ne 80 -r
python3 -W ignore main_0625_clean_05_blur_20180905.py --lr 0.001 --bs 128 --mode 1 --ne 30 -r
python3 -W ignore main_0625_clean_05_blur_20180905.py --mode 0 --network ckpt_20180905_0625_clean_05_blur.t0
