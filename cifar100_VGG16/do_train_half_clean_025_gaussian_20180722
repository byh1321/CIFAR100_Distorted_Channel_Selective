# train 0.125 from pruned, qunted half
python3 -W ignore network_gen.py --block1 ckpt_20180722_half_clean_016_gaussian_prune_80_pprec_15.t0 --o ckpt_20180722_half_clean_025_gaussian.t0 --mode 3
python3 -W ignore prune_quant_auto_half_clean_025_gaussian_20180722.py --lr 0.8 --bs 1024 --mode 1 --ne 150 --outputfile ckpt_20180722_half_clean_025_gaussian.t0
python3 -W ignore prune_quant_auto_half_clean_025_gaussian_20180722.py --lr 0.08 --bs 1024 --mode 1 --ne 150 -r --outputfile ckpt_20180722_half_clean_025_gaussian.t0
python3 -W ignore prune_quant_auto_half_clean_025_gaussian_20180722.py --lr 0.008 --bs 1024 --mode 1 --ne 150 -r --outputfile ckpt_20180722_half_clean_025_gaussian.t0
python3 -W ignore prune_quant_auto_half_clean_025_gaussian_20180722.py --lr 0.001 --bs 128 --mode 1 --ne 150 -r --outputfile ckpt_20180722_half_clean_025_gaussian.t0
python3 -W ignore prune_quant_auto_half_clean_025_gaussian_20180722.py --mode 0
