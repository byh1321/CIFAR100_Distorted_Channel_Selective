python -W ignore main_full_B3.py --mode 0 -r --blur 0.9
python -W ignore main_full_B3.py --mode 0 -r --blur 0.675
python -W ignore main_full_B3.py --mode 0 -r --blur 0.45
python -W ignore main_full_B3.py --mode 0 -r
python -W ignore main_full_B3.py --mode 0 -r --gau 0.08 
python -W ignore main_full_B3.py --mode 0 -r --gau 0.16
python -W ignore main_full_B3.py --mode 0 -r --gau 0.25
echo "Acc B3"

