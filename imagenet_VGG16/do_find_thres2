python FindPFSUMThres.py --min 0 --max 1000 --interval 1 --input1 PFSUM2_blur_3.txt --input2 PFSUM2_blur_15.txt
echo "blur3 and blur15"
python FindPFSUMThres.py --min 50000 --max 100000 --interval 100 --input1 PFSUM2_blur_15.txt --input2 PFSUM2_clean.txt
echo "blur15 and clean"
python FindPFSUMThres.py --min 500000 --max 1000000 --interval 1000 --input1 PFSUM2_clean.txt --input2 PFSUM2_gau_03.txt
echo "clean and gau03"
python FindPFSUMThres.py --min 1500000 --max 2000000 --interval 1000 --input1 PFSUM2_gau_03.txt --input2 PFSUM2_gau_06.txt
echo "gau_03 and gau06"
